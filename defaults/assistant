assistant.process
 assistant.listProcess:/bin/ps -u
 assistant.killProcess
  assistant.killProcessNumber:/bin/kill %d|assistant.param.process
  assistant.killProcessName:/usr/bin/killall %s|assistant.param.killProcessName
assistant.programs
 assistant.execute:/bin/%s|assistant.param.execute
 assistant.executeLocal:./%s|assistant.param.execute
assistant.users
 assistant.changePassword:/usr/bin/passwd
assistant.periphManage
 assistant.mountPeriph:/bin/mount %s %s|assistant.param.mountPeriph|assistant.param.mountTarget
assistant.files
 assistant.currentDirectory:/bin/pwd
 assistant.changeDirectory:cd %s|assistant.param.targetDirectory
 assistant.listSomeDirectory:/bin/ls %s|assistant.param.folderToList
 assistant.createFile:/bin/touch %s|assistant.param.fileToCreate
 assistant.editFile:/bin/nano %s|assistant.param.fileToEdit
assistant.reboot:reboot
assistant.setConfig:setConfig
assistant.exit
 assistant.confirmExit:/bin/exit
