assistant.process
 assistant.listProcess:/bin/ps -u
 assistant.killProcess
  assistant.killProcessNumber:/bin/kill %d
  assistant.killProcessName:/bin/killall %s
assistant.programs
 assistant.execute:/bin/%s
 assistant.executeLocal:./%s
assistant.users
 assistant.changePassword:/bin/passwd
assistant.periphManage
 assistant.mountPeriph:/bin/mount %s %s
assistant.files
 assistant.currentDirectory:/bin/pwd
 assistant.listSomeDirectory:/bin/ls %s
 assistant.createFile:/bin/touch %s
 assistant.editFile:/bin/nano %s
